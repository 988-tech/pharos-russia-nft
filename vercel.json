{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "static/**/*", "use": "@vercel/static" }
  ],
  "functions": {
    "api/**/*.py": { "runtime": "python3.11" }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/(.*)", "destination": "/api/index.py" }
  ]
}


