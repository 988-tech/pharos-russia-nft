# Копируйте этот файл в .env и заполните значения

# Приватный ключ кошелька для деплоя контракта и управления
PRIVATE_KEY=0xВАШ_ПРИВАТНЫЙ_КЛЮЧ

# RPC URL для PHAROS сети
PHAROS_RPC=https://rpc.testnet.pharos.network

# Адрес развёрнутого контракта (заполняется после деплоя)
CONTRACT_ADDRESS=0x73b8f9604c7063f6845e6c83763660232ada4bac

# Базовый URL приложения (по желанию). На Vercel не обязателен
# APP_BASE_URL=https://YOUR-APP.vercel.app

# Адрес для вывода PHRS средств с контракта
WITHDRAW_ADDRESS=YOUR_WALLET_ADDRESS_HERE

# Секретный ключ для Flask сессий
SESSION_SECRET=pharos-russia-nft-secret-key-change-in-production
